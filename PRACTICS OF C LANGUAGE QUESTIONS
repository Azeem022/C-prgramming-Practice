QUESTION 1:
// FIND ASCII VALUE OF ANY VARIABLE
#include<stdio.h>
int main()
{
	char variable;
	printf("enter the variable:");
	scanf("%c",&variable);
	printf("%d",variable);
	return 0;
}
QUESTION 2:
//QUESTION USING SWITCH CASE:
#include<stdio.h>
int main()
{
	int weeks;
	printf("entre weeks numbers from (1-7):");
	scanf("%d",&weeks);
	switch(weeks)
	{
		case 1:
		printf("monday");
		break;
		case 2:
		printf("tuesday");
		break;
		case 3:
		printf("wednesday");
		break;
		case 4:
		printf("thrusday");
		break;
		case 5:
		printf("friday");
		break;
		case 6:
		printf("saturday");
		break;
		case 7:
		printf("sunday");
		break;
		default:
		printf("invalid number plzz try again with valid number");
	}
	return 0;
}
QUESRION 3:
// CONCONENT OR VOVELS:
   #include<stdio.h>
int main()
{
	char alphabet;
	printf("entre the alphabet:");
	scanf("%c",&alphabet);
    switch(alphabet)
    {
    case 'a':
   	printf("vovel");
    break;
    case 'e':
   	printf("vovel");
    break;
    case 'i':
   	printf("vovel");
    break;
    case 'o':
   	printf("vovel");
    break;
    case 'u':
   	printf("vovel");
    break;
    case 'A':
   	printf("vovel");
    break;
    case 'E':
   	printf("vovel");
    break;
    case 'I':
   	printf("vovel");
    break;
    case 'O':
   	printf("vovel");
    break;
    case 'U':
   	printf("vovel");
    break;
    default:
    printf("conconent");
    }
	return 0;
}
QUWSTION 4:
// GREATEST NUMBER BETWEEN NUM1 OR NUM2:
#include<stdio.h>
int main()
{
	int num1, num2;
	printf("entre the num1:");
	scanf("%d",&num1);
	printf("entre the num2:");
	scanf("%d",&num2);
	switch(num1>num2)
	{
		case 1:
		printf("%d is greatest:",num1);
		break;
		default:
		printf("%d is greatest:",num2);
		break;	
	}
	return 0;
}
